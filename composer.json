{
    "name": "unocha/indicatorregistry8-site",
    "description": "Registry Indicator 8",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "UNOCHA",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.0",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_denied": "^1.1",
        "drupal/coder": "^8.3",
        "drupal/components": "^2.4",
        "drupal/config_split": "^2.0.0-beta4",
        "drupal/core-composer-scaffold": "^9.2.4",
        "drupal/core-dev": "^9.2.4",
        "drupal/core-project-message": "^9.2.4",
        "drupal/core-recommended": "^9.2.4",
        "drupal/csv_serialization": "^2.0",
        "drupal/ctools": "^3.7",
        "drupal/dream_block_manager": "^1.0",
        "drupal/facets": "^1.8",
        "drupal/facets_pretty_paths": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/google_tag": "^1.4",
        "drupal/maintenance200": "^1.0",
        "drupal/metatag": "^1.14",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.4",
        "drupal/search_api": "^1.15",
        "drupal/search_api_solr": "^4.1",
        "drupal/seckit": "^2.0",
        "drupal/select2": "^1.8",
        "drupal/social_auth_hid": "^2.6",
        "drupal/views_data_export": "^1.0",
        "drush/drush": "^10.3",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "phpoffice/phpspreadsheet": "^1.11",
        "unocha/common_design": "^4.0.0",
        "unocha/ocha_integrations": "^1.0",
        "webflo/drupal-finder": "^1.0.0"
    },
    "require-dev": {
        "drupal/config_filter": "^2.2",
        "drupal/console": "^1.0.2",
        "drupal/devel": "^4.1",
        "drupal/devel_php": "^1.1",
        "drupal/stage_file_proxy": "^1.1",
        "drupal/upgrade_rector": "^1.0@alpha",
        "drupal/upgrade_status": "^3.10",
        "kint-php/kint": "^3.3",
        "phpmd/phpmd": "^2.6",
        "phpspec/prophecy-phpunit": "^2",
        "webmozart/path-util": "^2.3",
        "weitzman/drupal-test-traits": "^1.2",
        "weitzman/logintrait": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "vendor/bin/",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal-composer/preserve-paths": true,
            "drupal/core-composer-scaffold": true,
            "drupal/console-extend-plugin": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::removeUnnecessaryFiles",
            "git config core.hooksPath git-hooks || true"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::removeUnnecessaryFiles",
            "git config core.hooksPath git-hooks || true"
        ],
        "sass-prepare": "cd html/themes/custom/common_design_subtheme && npm install",
        "sass-build": "cd html/themes/custom/common_design_subtheme && ./node_modules/.bin/sass sass:build",
        "sass-cleanup": "cd html/themes/custom/common_design_subtheme && rm -rf ./node_modules",
        "sass": [
            "@sass-prepare",
            "@sass-build",
            "@sass-cleanup"
        ]
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches-file": "composer.patches.json",
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "html/core": ["type:drupal-core"],
            "html/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "html/modules/contrib/{$name}": ["type:drupal-module"],
            "html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php",
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            },
            "locations": {
                "web-root": "html/"
            },
            "omit-defaults": false
        },
        "merge-plugin": {
            "include": [
                "html/core/composer.json"
            ],
            "recurse": true,
            "replace": false,
            "merge-extra": false
        }
    }
}
